  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": true,
    "stepDefinitions": "./cypress/e2e",
    "cucumberJson": {
      "generate": true,
      "screenshot": true,
      "outputFolder": "cypress/cucumber-json",
      "filePrefix": "",
      "fileSuffix": ".cucumber"
    }
  },
  "cypress": {
    "video": true,
    "autoCancelAfterFailures": false
  }
