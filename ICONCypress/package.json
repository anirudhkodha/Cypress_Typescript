  "scripts": {
    "precy:run": "shx rm -rf cypress/screenshots cypress/results cypress/reports || shx true",
    "cy:open": "npx cypress-ntlm open",
    "cy:run:spec1": "npx cypress-ntlm run --spec \"cypress/e2e/INCIDENT_Edit.feature\" --browser chrome",
    "cy:run:spec2": "npx cypress-ntlm run --env TAGS=\"@FullIncidentBushfiretype\" --spec \"cypress/e2e/INCIDENTS_Create.feature\"--browser chrome",
    "cy:run:spec3": "npx cypress-ntlm run --env TAGS=\"@Teamcity\" --record --key 7699feeb-e089-4d7b-8116-9fb4ce90d95d --spec \"cypress/e2e/Admin.feature\" --browser edge",
    "cy:run:spec4": "npx cypress-ntlm run --env TAGS=\"@Teamcity\" --record --key 7699feeb-e089-4d7b-8116-9fb4ce90d95d --spec \"cypress/e2e/operationTAB_pageView.feature\" --browser edge",
    "cy:run:spec5": "npx cypress-ntlm run --env TAGS=\"@Teamcity\" --record --key 7699feeb-e089-4d7b-8116-9fb4ce90d95d --spec \"cypress/e2e/ICON_activitySum.feature\" --browser edge",
    "cy:run:spec6": "npx cypress-ntlm run --env TAGS=\"@Teamcity\" --record --key 7699feeb-e089-4d7b-8116-9fb4ce90d95d --spec \"cypress/e2e/Inc_EVNT_OPS_LOG.feature\" --browser edge",
    "cy:run:spec7": "npx cypress-ntlm run --env TAGS=\"@Teamcity\" --record --key 7699feeb-e089-4d7b-8116-9fb4ce90d95d --spec \"cypress/e2e/Planning_TAB_Pages.feature\" --browser edge",
    "cy:run:spec8": "npx cypress-ntlm run --env TAGS=\"@Teamcity\" --record --key 7699feeb-e089-4d7b-8116-9fb4ce90d95d --spec \"cypress/e2e/SitRepSitUp.feature\" --browser edge",
    "cy:run:spec9": "npx cypress-ntlm run --env TAGS=\"@Teamcity\" --record --key 7699feeb-e089-4d7b-8116-9fb4ce90d95d --spec \"cypress/e2e/Create-evt.feature\" --browser edge",
    "cy:run:spec10": "npx cypress-ntlm run --env TAGS=\"@Teamcity\" --record --key 7699feeb-e089-4d7b-8116-9fb4ce90d95d --spec \"cypress/e2e/Incidents.feature\" --browser edge",
    "cy:run:spec11": "npx cypress-ntlm run --env TAGS=\"@Teamcity\" --record --key 7699feeb-e089-4d7b-8116-9fb4ce90d95d --spec \"cypress/e2e/S44.feature\" --browser edge",
    "cy:run:spec12": "npx cypress-ntlm run --env TAGS=\"@Teamcity\" --record --key 7699feeb-e089-4d7b-8116-9fb4ce90d95d --spec \"cypress/e2e/BIRS.feature\" --browser edge",
    "cy:run:spec13": "npx cypress-ntlm run --record --key ab96c532-b1ba-427b-9315-17682c3f4952 --spec \"cypress/e2e/Serverpatch.feature\" --browser chrome",
    "cy:run": "npm run cy:run:spec1 && .......",
    "report": "run-s report:*",
    "report:merge": "mochawesome-merge cypress/results/json/*.json > cypress/results/mochawesome-bundle.json",
    "report:generate": "marge cypress/results/mochawesome-bundle.json -o cypress/reports -f cypress-report.html -i true",
    "report:copyScreenshots": "shx --silent cp -r cypress/screenshots cypress/reports/screenshots || shx true"
  },
